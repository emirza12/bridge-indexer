# RPC provider URLs
HOLESKY_RPC_URL=https://ethereum-holesky.publicnode.com
TARGET_CHAIN_RPC_URL=https://rpc.sepolia.org

# WebSocket provider URLs
HOLESKY_WS_URL=wss://ethereum-holesky.publicnode.com
TARGET_CHAIN_WS_URL=wss://sepolia.infura.io/ws/v3/YOUR_INFURA_KEY

# Contract addresses
HOLESKY_BRIDGE_ADDRESS=0x...
TARGET_CHAIN_BRIDGE_ADDRESS=0x...

# Token addresses (must be supported in both bridge contracts)
HOLESKY_TEST_TOKEN=0x...
TARGET_CHAIN_TEST_TOKEN=0x...

# Confirmation Thresholds
HOLESKY_CONFIRMATION_BLOCKS=15
TARGET_CHAIN_CONFIRMATION_BLOCKS=6

# Private key for signing transactions (KEEP THIS SECURE!)
# Must be from an account that has OWNER permissions on bridge contracts
PRIVATE_KEY=your_private_key_here

# Database URL
DATABASE_URL=postgres://username:password@localhost:5432/bridgeindexer 